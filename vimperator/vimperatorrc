set runtimepath=~/.config/vimperator

hi StatusLine color: black;
set gui=nonavigation

nnoremap h H
nnoremap H h
nnoremap l L
nnoremap L l
nnoremap gh gT
nnoremap gl gt
nnoremap gt <C-^>

let mapleader = ","

command -nargs=0 interwiki javascript content.location.href = io.system("interwiki", content.location.href)
nnoremap <leader>z :interwiki<cr>
nnoremap <leader>gd :tabduplicate<cr>
